!!! 5
html(lang="en")
  head
    script(type='text/javascript')
      window.pPort = '#{port}';
      window.pHost = '#{host}';
    - if (p_debug)
      script(src="js/jquery-1.5.1.js", type="text/javascript")
    - else
      script(src="js/jquery-1.5.1.min.js", type="text/javascript")
    script(src="js/jquery.ba-hashchange.min.js", type="text/javascript")
    script(src="js/underscore-min.js", type="text/javascript")
    script(src="js/json2.js", type="text/javascript")
    script(src="js/backbone.js", type="text/javascript")
    script(src="/socket.io/socket.io.js", type="text/javascript")
    script(src="js/procrastinator.js", type="text/javascript")
    link(rel='stylesheet', href="css/procrastinator.css", type="text/css")
    link(rel='stylesheet', href="css/blueprint/screen.css", type="text/css", media="screen, projection")
    link(rel='stylesheet', href="css/blueprint/print.css", type="text/css", media="print")
    /if lt IE 8
      link(rel='stylesheet', href="css/blueprint/ie.css", type="text/css", media="screen, projection")
    title Mainline pictures of cats
  body
    - if (p_debug)
      #content(class='showgrid container')
        !=body
    - else
      #content(class='container')
        !=body
